{
  "name": "ts-results-es",
  "version": "3.6.0",
  "description": "A typescript implementation of Rust's Result and Option objects.",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./rxjs-operators": {
      "types": "./dist/rxjs-operators/index.d.ts",
      "import": "./dist/esm/rxjs-operators/index.mjs",
      "require": "./dist/cjs/rxjs-operators/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup && tsup --dts-only --outDir dist && npm run build:copy",
    "build:copy": "copyfiles README.md LICENSE src/package.json dist --flat",
    "clean": "rm -rf dist",
    "test": "jest",
    "format": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lune-climate/ts-results-es.git"
  },
  "keywords": [],
  "author": "Vultix",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "conditional-type-checks": "^1.0.5",
    "copyfiles": "^2.4.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rxjs": "^7.8.0",
    "ts-jest": "^26.5.4",
    "tslib": "^2.3.1",
    "tsup": "^6.7.0",
    "typescript": "^4.6.3"
  }
}
